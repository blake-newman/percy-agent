{
  "name": "@percy/agent",
  "description": "A low-level Node process for interacting with Percy.",
  "version": "0.0.1",
  "author": "Perceptual Inc",
  "bin": {
    "percy-agent": "./bin/run"
  },
  "watch": {
    "build": {
      "patterns": ["src/percy-agent-client/*.ts"],
      "extensions": "ts",
      "legacyWatch": true,
      "delay": 1000
    }
  },
  "bugs": "https://github.com/percy/percy-agent/issues",
  "dependencies": {
    "@oclif/command": "^1.4.21",
    "@oclif/config": "^1.6.16",
    "@oclif/plugin-help": "^1.2.7",
    "@oclif/plugin-not-found": "^1.0.7",
    "@types/express": "^4.11.1",
    "@types/sinon": "^4.3.1",
    "@types/winston": "^2.3.9",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "npm-watch": "^0.3.0",
    "percy-client": "^2.8.2",
    "retry-axios": "^0.3.2",
    "sinon": "^4.5.0",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.19",
    "@oclif/errors": "^1.0.9",
    "@oclif/test": "^1.0.5",
    "@oclif/tslint": "^1.1.1",
    "@types/chai": "^4.1.3",
    "@types/chai-http": "^3.0.4",
    "@types/cors": "^2.8.4",
    "@types/mocha": "^5.2.0",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.13",
    "@types/sinon-chai": "^2.7.29",
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "globby": "^8.0.1",
    "husky": "^1.0.0-rc.3",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "prettier": "1.12.1",
    "pryjs": "^1.0.3",
    "sinon-chai": "^3.0.0",
    "stdout-stderr": "^0.1.9",
    "ts-node": "5",
    "tslib": "^1.9.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "package.json",
    "/bin",
    "/dist"
  ],
  "homepage": "https://github.com/percy/percy-agent",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "oclif",
    "percy",
    "agent"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "percy-agent",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ]
  },
  "repository": "percy/percy-agent",
  "scripts": {
    "build": "rm -rf dist && tsc && yarn run build-percy-agent-client",
    "build-percy-agent-client": "rm -rf dist/public && tsc -p src/percy-agent-client",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p test --noEmit && tslint -p test -t stylish --fix",
    "postpublish": "yarn run clean",
    "posttest": "yarn run lint",
    "prepublishOnly": "yarn run build && oclif-dev manifest && oclif-dev readme",
    "preversion": "yarn run clean",
    "test": "yarn run build-percy-agent-client && PERCY_TOKEN=abc PERCY_PROJECT=test/test mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "watch": "npm-watch"
  },
  "types": "dist/index.d.ts"
}
